<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_notif"
    app:startDestination="@id/donateFragment">
    <fragment
        android:id="@+id/donateFragment"
        android:name="abdellah.project.fooddonation.fragment.DonateFragment"
        android:label="fragment_donate"
        tools:layout="@layout/fragment_donate" >
        <action
            android:id="@+id/action_donateFragment_to_myHelpFragment2"
            app:destination="@id/myHelpFragment2" />
        <action
            android:id="@+id/action_donateFragment_to_fragment_recieve_2"
            app:destination="@id/fragment_recieve_2" />
    </fragment>
    <fragment
        android:id="@+id/myHelpFragment2"
        android:name="abdellah.project.fooddonation.fragment.MyHelpFragment"
        android:label="fragment_my_help"
        tools:layout="@layout/fragment_my_help" >
        <action
            android:id="@+id/action_myHelpFragment2_to_donateFragment"
            app:destination="@id/donateFragment" />
        <action
            android:id="@+id/action_myHelpFragment2_to_mapsFragment2"
            app:destination="@id/mapsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/mapsFragment2"
        android:name="abdellah.project.fooddonation.fragment.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/fragment_recieve_2"
        android:name="abdellah.project.fooddonation.fragment.Fragment_recieve_"
        android:label="fragment_recieve_"
        tools:layout="@layout/fragment_recieve_" />
</navigation>